{
  "name": "nextee",
  "version": "0.0.0",
  "source": "src/main.ts",
  "types": "dist/**/*.ts",
  "description": "nextee tenplate initializer",
  "repository": "https://github.com/mitch1009/nextee-cli.git",
  "homepage": "https://cli.nextee.site",
  "author": {
    "name": "Mitch Chanza",
    "email": "heloo@mitch.guru",
    "url": "https://www.mitch.guru"
  },
  "contributors": [
    {
      "name": "Mitch Chanza",
      "email": "hello@mitch.guru",
      "url": "https://www.mitch.guru",
      "organization": "korena"
    }
  ],
  "main": "out/main.js",
  "module": "out/main.js",
  "type": "module",
  "alias": {
    "@": "./src"
  },
  "bin": {
    "nextee": "out/main.js"
  },
  "files": [
    "out/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "doc": "jsdoc -c jsdoc.json src/main.ts",
    "build": "tsc",
    "tsg": "node dist/execute.js",
    "prepublish": "pnpm build",
    "nextee": "node ./out/main.js",
    "pub": "changeset publish",
    "pb": "parcel build",
    "test": "jest"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "nextee",
    "nextjs latest",
    "Typescript",
    "nodejs",
    "tailwincss",
    "prisma"
  ],
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@parcel/compressor-brotli": "^2.12.0",
    "@parcel/compressor-gzip": "^2.12.0",
    "@parcel/config-default": "^2.12.0",
    "@types/node": "^20.12.4",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "better-docs": "^2.7.3",
    "env-cmd": "^10.1.0",
    "express": "^4.19.2",
    "jsdoc": "^4.0.2",
    "nodemon": "^3.1.0",
    "parcel": "^2.12.0",
    "parcel-resolver-tspaths": "^0.0.9",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  }
}